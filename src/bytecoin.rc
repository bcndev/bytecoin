            VALUE "CompanyName", "Bytecoin Developers"
            VALUE "FileDescription", "Bytecoin"
            VALUE "FileVersion", BYTECOIN_VERSION_WINDOWS_STRING
            VALUE "InternalName", "bytecoin.rc"
