            VALUE "OriginalFilename", "bytecoin.rc"
            VALUE "ProductName", "Bytecoin"
            VALUE "ProductVersion", BYTECOIN_VERSION_WINDOWS_STRING
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END

// Icon with lowest ID value placed first to ensure application icon
// remains consistent on all systems.
101               ICON                    "bytecoin.ico"
